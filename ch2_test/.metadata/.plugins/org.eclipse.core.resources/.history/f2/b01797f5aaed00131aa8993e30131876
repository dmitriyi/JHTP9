package ch2_Me_Print;

public class Excersize31 {
	public static void main (String [] args){
		int num, y, x, z;
		num = 0;
		
				
		System.out.printf("Number    Square    Cube", num);
		
		if (num < 10){
			y = num + 1;
			x = y * y;
			z = y * y * y;
						System.out.printf("%d     %d     %d", num, x, z); 
		} else {
			System.out.print("done");
		}
	}
}
