import java.io.*;

public class OutPrint3 {
	//Class Declaration - Named Out Print
	
	public static void main (String[] args){
		//One of the methods in class must be called main!!!
	// Void means - this method will not return information\
	
		System.out.printf( "%s\n%s\n", "Welcome to Java", " Programming!" );
		//System.out.println("Welcome to Java Programming!" );
		
	}

}
