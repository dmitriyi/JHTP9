package ch2_Me_Print;

public class Excersize31 {
	public static void main (String [] args){
		int n0 = 0;
		int n1 = 1;
		int n2 = 2;
		int n3 = 3;
		int n4 = 4;
		int n5 = 5;
		int n6 = 6;
		int n7 = 7;
		int n8 = 8;
		int n9 = 9;
		int n10 = 10;
		
		System.out.printf("Number    Square    Cube\n");
		System.out.printf("%d        %d        %d\n", n0, n0*n0, n0*n0*n0);
		System.out.printf("%d        %d        %d\n", n1, n1*n1, n1*n1*n1);
		System.out.printf("%d        %d        %d\n", n2, n2*n2, n2*n2*n2);
		System.out.printf("%d        %d        %d\n", n3, n3*n3, n3*n3*n3);
		System.out.printf("%d        %d        %d\n", n4, n4*n4, n4*n4*n4);
		System.out.printf("%d        %d        %d\n", n5, n5*n5, n5*n5*n5);
		System.out.printf("%d        %d        %d\n", n6, n6*n6, n6*n6*n6);
		System.out.printf("%d        %d        %d\n", n7, n7*n7, n7*n7*n7);
		System.out.printf("%d        %d        %d\n", n8, n8*n8, n8*n8*n8);
		System.out.printf("%d        %d        %d\n", n9, n9*n9, n9*n9*n9);
		System.out.printf("%d        %d        %d\n", n10, n10*n10, n10*n10*n10);
		
		
		
		
		
	}
}
